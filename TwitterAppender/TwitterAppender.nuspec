    <licenseUrl>https://github.com/russellchadwick/TwitterAppender/blob/master/LICENSE</licenseUrl>
    <tags>Twitter Appender Log4Net funny</tags>
    <language>en-US</language>
    <copyright>Russell Chadwick 2012</copyright>
    <dependencies>
      <dependency id="LinqToTwitter" version="[2.1.8]" />
      <dependency id="log4net" version="[2.0.2]" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\russellchadwick.Appenders.dll" target="lib\net40\russellchadwick.Appenders.dll" />
    <file src="**\*.cs" target="src" />
  </files>
</package>